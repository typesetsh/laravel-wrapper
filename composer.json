{
    "name": "typesetsh/laravel-wrapper",
    "description": "Typeset.sh wrapper for laravel 7/8",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Siefer",
            "email": "jacob@typeset.sh"
        },
        {
            "name": "Rouven Alexander Rieker",
            "email": "therouv@googlemail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "^7.0 || ^8.0",
        "typesetsh/typesetsh": "^0.14"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Typesetsh\\LaravelWrapper\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Typesetsh\\LaravelWrapper\\TypesetServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.typeset.sh"
        }
    ]
}
